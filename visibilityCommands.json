        "cmd": "curl -s https://ipinfo.io/"
    },
	   
    "getCPE": 
    {
        "type": "run",
        "desc": "discover.T0846.001: TCP SysInfo Discovery--> tcp_fingerprint [target_ip]", 
        "cmd": "sudo nmap -sV -O"
    },
   "get_cpe_name": 
    {
        "type": "run",
        "desc": "Get CPE name", 
        "cmd": "python3 ./third_party/get_cpe_name.py"
    },
   "chat": 
    {
