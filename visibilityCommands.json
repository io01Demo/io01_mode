        "cmd": "sudo ./third_party/self_discover.sh"
    },
	"passive_scan": {
        "type": "run",
        "desc": "Sniff local network [interface] for a [duration] and provides basic information", 
        "cmd": "sudo ./third_party/passive_scan_sniff.sh"
    },
	"arp_scan": {
        "type": "run",
        "desc": "Use arp scan to map [network]", 
